{
  "name": "sfdc-eventmonitoring",
  "version": "0.1.5",
  "description": "Command line tools surrounding event monitoring",
  "main": "eventmonitoring.js",
  "bin": {
    "eventmonitoring": "eventmonitoring.js"
  },
  "scripts": {
    "lint-eslint": "eslint eventmonitoring.js src/*",
    "lint-md": "remark --no-stdout -S README.md",
    "lint": "npm run lint-eslint && npm run lint-md",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "remark-lint-final-newline",
        false
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pcon/sfdc-eventMonitoring.git"
  },
  "keywords": [
    "salesforce",
    "event",
    "monitoring"
  ],
  "author": "Patrick Connelly",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/pcon/sfdc-eventMonitoring/issues"
  },
  "homepage": "https://github.com/pcon/sfdc-eventMonitoring#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "csvtojson": "^2.0.4",
    "ini": "^1.3.5",
    "jsforce": "^1.8.4",
    "jsonfile": "^4.0.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "pretty-bytes": "^5.1.0",
    "pretty-ms": "^3.2.0",
    "q": "^1.5.1",
    "table": "^4.0.3",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "eslint": "^5.0.1",
    "remark-lint": "^6.0.2",
    "remark-preset-lint-recommended": "^3.0.2"
  }
}